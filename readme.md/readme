def _load_image(path, img_size):
    img = tf.io.read_file(path)
    try:
        img = tf.image.decode_image(img, channels=3, expand_animations=False)
        img.set_shape([None, None, 3])  # ‚úÖ Force shape
        img = tf.image.resize(img, img_size)
