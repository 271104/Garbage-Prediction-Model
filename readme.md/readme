update this dataset_tools.py import os
import csv
import glob
from tkinter import Tk, Label, Scale, HORIZONTAL, Button, filedialog, StringVar
from PIL import Image, ImageTk

class GarbageDatasetPreparer:
    def __init__(self, root="data"):
        self.root = root
